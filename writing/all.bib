  author = {Legendre, Pierre and Anderson, Marti J.},
  file = {/home/brendan/Documents/Uppsala/Documents/References/Statistics/CCA,RDA,PCoA/Legendre_Anderson_1999_Distance-based_redundancy_analysis(Ecological_Monographs).pdf}
