@article{Averill2016,
  title = {Ectomycorrhizal Fungi Slow Soil Carbon Cycling},
  volume = {19},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/ele.12631},
  doi = {10.1111/ele.12631},
  abstract = {Abstract Respiration of soil organic carbon is one of the largest fluxes of CO2 on earth. Understanding the processes that regulate soil respiration is critical for predicting future climate. Recent work has suggested that soil carbon respiration may be reduced by competition for nitrogen between symbiotic ectomycorrhizal fungi that associate with plant roots and free‐living microbial decomposers, which is consistent with increased soil carbon storage in ectomycorrhizal ecosystems globally. However, experimental tests of the mycorrhizal competition hypothesis are lacking. Here we show that ectomycorrhizal roots and hyphae decrease soil carbon respiration rates by up to 67\% under field conditions in two separate field exclusion experiments, and this likely occurs via competition for soil nitrogen, an effect larger than 2 °C soil warming. These findings support mycorrhizal competition for nitrogen as an independent driver of soil carbon balance and demonstrate the need to understand microbial community interactions to predict ecosystem feedbacks to global climate.},
  number = {8},
  journaltitle = {Ecology Letters},
  shortjournal = {Ecol. Lett.},
  date = {2016},
  pages = {937-947},
  keywords = {Biogeochemistry,ecosystems,mycorrhizae,soil carbon,soil ecology,soil nitrogen},
  author = {Averill, Colin and Hawkes, Christine V.},
  file = {/home/brendan/.mozilla/firefox/uzm114dr.default/zotero/storage/TBPBVIZN/Averill and Hawkes - 2016 - Ectomycorrhizal fungi slow soil carbon cycling.pdf;/home/brendan/Documents/Uppsala/Documents/Papers/all/Averill and Hawkes 2016 Ectomycorrhizal fungi slow soil carbon cycling.pdf},
