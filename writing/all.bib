@article{porterEcosystemFunctionsMine2008,
  title = {Ecosystem Functions within a Mine Drainage Passive Treatment System},
  volume = {32},
  issn = {0925-8574},
  url = {http://www.sciencedirect.com/science/article/pii/S092585740800013X},
  doi = {10.1016/j.ecoleng.2007.12.013},
  abstract = {The impacts of acid mine drainage (AMD) on receiving water bodies have been extensively studied. Water quality improvement provided by constructed wetlands is well known, but little is known about the ecological function of these treatment systems. A passive treatment system was constructed in eastern Oklahoma to treat AMD from an abandoned coal mine. Multiple water quality parameters were monitored. Chlorophyll a concentrations were determined throughout the system to estimate productivity. Multi-plate artificial substrate samplers were deployed to quantify and evaluate the macroinvertebrate assemblage within each cell. Productivity between treatment cells varied significantly. The cells receiving effluent from anoxic organic substrate had elevated nutrient levels, which led to elevated productivity. Chlorophyll a levels reached hypereutrophic conditions into final treatment cells. Macroinvertebrate community structure was significantly different among treatment cells. Primary cells produced more diverse and evenly distributed taxa, including the presence of moderately intolerant taxa. The treatment system exhibited early successional stage ecosystem functions and trends toward destructive oscillations.},
  number = {4},
  journaltitle = {Ecological Engineering},
  shortjournal = {Ecological Engineering},
  urldate = {2014-04-12},
  date = {2008-04-01},
  pages = {337-346},
  keywords = {Chlorophyll a,Coal mine drainage,ecosystem functions,macroinvertebrates},
  author = {Porter, Clint M. and Nairn, Robert W.},
  file = {/home/brendan/.mozilla/firefox/uzm114dr.default/zotero/storage/Z37G2QPW/Porter and Nairn - 2008 - Ecosystem functions within a mine drainage passive.pdf;/home/brendan/.mozilla/firefox/uzm114dr.default/zotero/storage/4B929ASU/S092585740800013X.html;/home/brendan/.mozilla/firefox/uzm114dr.default/zotero/storage/DHMXE7TX/S092585740800013X.html}
