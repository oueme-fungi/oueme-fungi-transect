@article{lopezTESTINGFLORISTICQUALITY2002,
  title = {{{TESTING THE FLORISTIC QUALITY ASSESSMENT INDEX AS AN INDICATOR OF WETLAND CONDITION}}},
  volume = {12},
  issn = {1051-0761},
  url = {http://www.esajournals.org/doi/abs/10.1890/1051-0761(2002)012%5B0487:TTFQAI%5D2.0.CO%3B2},
  doi = {10.1890/1051-0761(2002)012[0487:TTFQAI]2.0.CO;2},
  abstract = {Biological indicators of ecosystem integrity are increasingly being sought for use in ecosystem assessment and goal-setting for restoration projects. We tested the effectiveness of a plant community-based bioassessment tool, the floristic quality assessment index (FQAI) in 20 depressional wetlands in Ohio, USA. A priori, the 20 depressional wetlands were classified by type and ranked to form a disturbance gradient according to the local landscape condition. Ranks were based on surrounding land cover characteristics, vegetated buffer characteristics, and the extent of human-induced hydrologic alteration at the wetland site. The index was negatively correlated with the disturbance rank of a wetland and with the distance to neighboring wetlands (P = 0.01). Index values were lower for wetlands surrounded by agricultural or urban land use, wetlands with less vegetation on the wetland perimeter, and wetlands with more hydrologic modification, and at sites with greater distances to other wetlands. The wetlands with lower FQAI values tended to be dominated by plants that are typical of heavily cultivated landscapes or urban regions. Thus, the index is interpreted as a measure of environmental factors that maintain and control plant communities. The index was not correlated with differences in wetland surface water chemistry (α = 0.05) but was positively correlated with soil total organic carbon (P = 0.01), phosphorus (P = 0.05), and calcium (P = 0.05). Repeated wetland sampling in the summer and autumn revealed that the floristic quality assessment index could be useful for the assessment and monitoring of wetland ecosystems and for tracking wetland restoration projects over time.},
  number = {2},
  journaltitle = {Ecological Applications},
  shortjournal = {Ecological Applications},
  urldate = {2014-05-08},
  date = {2002-04-01},
  pages = {487-497},
  author = {Lopez, Ricardo D. and Siobhan Fennessy, M.},
  file = {/home/brendan/.mozilla/firefox/uzm114dr.default/zotero/storage/WJWC97IF/3060957.pdf;/home/brendan/.mozilla/firefox/uzm114dr.default/zotero/storage/I32BEHW4/1051-0761(2002)012[0487TTFQAI]2.0.html;/home/brendan/.mozilla/firefox/uzm114dr.default/zotero/storage/VFI5X6VU/1051-0761(2002)012[0487TTFQAI]2.0.html}
}

