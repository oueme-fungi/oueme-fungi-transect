  urldate = {2014-04-13},
  date = {2011-11-01},
  pages = {723-736},
  keywords = {Environmental Management,Nature Conservation,Phytoplankton,Road salt,Urban Ecology,Zooplankton,ecology,periphyton},
  author = {Meter, Robin J. Van and Swan, Christopher M. and Snodgrass, Joel W.},
  file = {/home/brendan/.mozilla/firefox/uzm114dr.default/zotero/storage/PUQAM29Q/Meter et al. - 2011 - Salinization alters ecosystem structure in urban s.pdf;/home/brendan/.mozilla/firefox/uzm114dr.default/zotero/storage/79BBWPSK/s11252-011-0180-9.html}
