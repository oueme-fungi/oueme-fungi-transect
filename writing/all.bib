  file = {/home/brendan/Documents/Uppsala/Documents/References/Koetschan_etal_2014_Internal_Transcribed_Spacer_1_Secondary_Structure_Analysis_Reveals_a_Common(PLoS_One).pdf},
  eprinttype = {pmid},
