@article{abarenkov2018a,
  langid = {english},
  title = {Protax-Fungi: A Web-Based Tool for Probabilistic Taxonomic Placement of Fungal Internal Transcribed Spacer Sequences},
  volume = {220},
  issn = {1469-8137},
  url = {http://nph.onlinelibrary.wiley.com/doi/abs/10.1111/nph.15301},
  doi = {10.1111/nph.15301},
  shorttitle = {Protax-Fungi},
  abstract = {Incompleteness of reference sequence databases and unresolved taxonomic relationships complicates taxonomic placement of fungal sequences. We developed Protax-fungi, a general tool for taxonomic placement of fungal internal transcribed spacer (ITS) sequences, and implemented it into the PlutoF platform of the UNITE database for molecular identification of fungi. With empirical data on root- and wood-associated fungi, Protax-fungi reliably identified (with at least 90\% identification probability) the majority of sequences to the order level but only around one-fifth of them to the species level, reflecting the current limited coverage of the databases. Protax-fungi outperformed the Sintax and Rdb classifiers in terms of increased accuracy and decreased calibration error when applied to data on mock communities representing species groups with poor sequence database coverage. We applied Protax-fungi to examine the internal consistencies of the Index Fungorum and UNITE databases. This revealed inconsistencies in the taxonomy database as well as mislabelling and sequence quality problems in the reference database. The according improvements were implemented in both databases. Protax-fungi provides a robust tool for performing statistically reliable identifications of fungi in spite of the incompleteness of extant reference sequence databases and unresolved taxonomic relationships.},
  number = {2},
  journaltitle = {New Phytologist},
  shortjournal = {New Phytol.},
  urldate = {2019-01-30},
  date = {2018},
  pages = {517-525},
  keywords = {fungi,annotation,data quality,environmental sequencing,identification tool,internal transcribed spacer (ITS),molecular species identification,probabilistic taxonomic assignment},
  author = {Abarenkov, Kessy and Somervuo, Panu and Nilsson, R. Henrik and Kirk, Paul M. and Huotari, Tea and Abrego, Nerea and Ovaskainen, Otso},
  file = {/home/brendan/Documents/Uppsala/Documents/References/Computer Methods/Bioinformatics/Abarenkov_etal_2018_Protax-fungi(New_Phytologist).pdf;/home/brendan/.mozilla/firefox/uzm114dr.default/zotero/storage/9SDNFJ2I/nph.html}
}

@article{he2019,
  langid = {english},
  title = {Genetic Basis for the Establishment of Endosymbiosis in {{Paramecium}}},
  issn = {1751-7370},
  url = {https://www.nature.com/articles/s41396-018-0341-4},
  doi = {10.1038/s41396-018-0341-4},
  abstract = {The single-celled ciliate Paramecium bursaria is an indispensable model for investigating endosymbiosis between protists and green-algal symbionts. To elucidate the mechanism of this type of endosymbiosis, we combined PacBio and Illumina sequencing to assemble a high-quality and near-complete macronuclear genome of P. bursaria. The genomic characteristics and phylogenetic analyses indicate that P. bursaria is the basal clade of the Paramecium genus. Through comparative genomic analyses with its close relatives, we found that P. bursaria encodes more genes related to nitrogen metabolism and mineral absorption, but encodes fewer genes involved in oxygen binding and N-glycan biosynthesis. A comparison of the transcriptomic profiles between P. bursaria with and without endosymbiotic Chlorella showed differential expression of a wide range of metabolic genes. We selected 32 most differentially expressed genes to perform RNA interference experiment in P. bursaria, and found that P. bursaria can regulate the abundance of their symbionts through glutamine supply. This study provides novel insights into Paramecium evolution and will extend our knowledge of the molecular mechanism for the induction of endosymbiosis between P. bursaria and green algae.},
  journaltitle = {The ISME Journal},
  shortjournal = {ISME J.},
  urldate = {2019-01-31},
  date = {2019-01-15},
  pages = {1},
  author = {He, Ming and Wang, Jinfeng and Fan, Xinpeng and Liu, Xiaohui and Shi, Wenyu and Huang, Ning and Zhao, Fangqing and Miao, Miao},
  file = {/home/brendan/Documents/Uppsala/Documents/References/Computer Methods/Bioinformatics/He_etal_2019_Genetic_basis_for_the_establishment_of_endosymbiosis_in_Paramecium(The_ISME_Journal).pdf;/home/brendan/.mozilla/firefox/uzm114dr.default/zotero/storage/UICKRLYY/s41396-018-0341-4.html}
}

