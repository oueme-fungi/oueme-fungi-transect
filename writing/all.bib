@article{bouchardEffectsMacrophyteFunctional2007,
  eprinttype = {jstor},
  eprint = {27651448},
  title = {Effects of {{Macrophyte Functional Group Richness}} on {{Emergent Freshwater Wetland Functions}}},
  volume = {88},
  issn = {0012-9658},
  abstract = {Most plant diversity—function studies have been conducted in terrestrial ecosystems and have focused on plant productivity and nutrient uptake/retention, with a notable lack of attention paid to belowground processes (e.g., root dynamics, decomposition, trace gas fluxes). Here we present results from a mesocosm experiment in which we assessed how the richness of emergent macrophyte functional groups influences aboveground and belowground plant growth and microbial-mediated functions related to carbon and nitrogen cycling, with an emphasis on methane (CH₄) efflux and potential denitrification rates. We found that an increase in the richness of wetland plant functional groups enhanced belowground plant biomass, altered rooting patterns, and decreased methane efflux, while having no effect on aboveground plant production or denitrification potential. We hypothesize that the greater root production and increased rooting depth in the highest diversity treatments enhanced CH₄ oxidation to a relatively greater degree than methane production, leading to an overall decrease in CH₄ efflux across our plant functional group richness gradient.},
  number = {11},
  journaltitle = {Ecology},
  shortjournal = {Ecology},
  date = {2007-11-01},
  pages = {2903-2914},
  author = {Bouchard, Virginie and Frey, Serita D. and Gilbert, Janice M. and Reed, Sharon E.},
  file = {/home/brendan/.mozilla/firefox/uzm114dr.default/zotero/storage/R8H3W4PJ/Bouchard et al. - 2007 - Effects of Macrophyte Functional Group Richness on.pdf;/home/brendan/.mozilla/firefox/uzm114dr.default/zotero/storage/IV6ABHAP/06-1144.html}
}

