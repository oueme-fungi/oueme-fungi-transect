@article{ledergerber2008,
  langid = {english},
  title = {Alignments with Non-Overlapping Moves, Inversions and Tandem Duplications in {{O}}(N4) Time},
  volume = {16},
  issn = {1382-6905, 1573-2886},
  url = {https://link.springer.com/article/10.1007/s10878-007-9132-y},
  doi = {10.1007/s10878-007-9132-y},
  abstract = {Sequence alignment is a central problem in bioinformatics. The classical dynamic programming algorithm aligns two sequences by optimizing over possible insertions, deletions and substitutions. However, other evolutionary events can be observed, such as inversions, tandem duplications or moves (transpositions). It has been established that the extension of the problem to move operations is NP-complete. Previous work has shown that an extension restricted to non-overlapping inversions can be solved in O(n 3) with a restricted scoring scheme. In this paper, we show that the alignment problem extended to non-overlapping moves can be solved in O(n 5) for general scoring schemes, O(n 4log n) for concave scoring schemes and O(n 4) for restricted scoring schemes. Furthermore, we show that the alignment problem extended to non-overlapping moves, inversions and tandem duplications can be solved with the same time complexities. Finally, an example of an alignment with non-overlapping moves is provided.},
  number = {3},
  journaltitle = {Journal of Combinatorial Optimization},
  shortjournal = {J Comb Optim},
  urldate = {2018-06-05},
  date = {2008-10-01},
  pages = {263-278},
  keywords = {Operations Research/Decision Theory,Optimization,Theory of Computation,Convex and Discrete Geometry,Combinatorics,Dynamic programming,String to string comparison,Block operations,Scoring schemes,Biological sequence alignment,modeling,sequence alignment,Bioinformatics},
  author = {Ledergerber, Christian and Dessimoz, Christophe},
  file = {/home/brendan/Documents/Uppsala/Documents/References/Computer Methods/Bioinformatics/Ledergerber_Dessimoz_2008_Alignments_with_non-overlapping_moves,_inversions_and_tandem_duplications_in(J_Comb_Optim).pdf;/home/brendan/.mozilla/firefox/uzm114dr.default/zotero/storage/QCC4IGEK/s10878-007-9132-y.html}
