  editor = {Keith, Jonathan M.},
  file = {/home/brendan/Documents/Uppsala/Documents/References/Computer Methods/Bioinformatics/Verma_etal_2017_Genome_Sequencing(Bioinformatics_Volume_I_Data,_Sequence_Analysis,_and_Evolution).pdf}
}

