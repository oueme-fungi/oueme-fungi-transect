  date = {2015},
  pages = {225-242},
  author = {Henras, Anthony K and Plisson-Chastang, Célia and O'Donohue, Marie-Françoise and Chakraborty, Anirban and Gleizes, Pierre-Emmanuel},
  file = {/home/brendan/Documents/Uppsala/Documents/References/rRNA/Henras_etal_2015_An_overview_of_pre-ribosomal_RNA_processing_in_eukaryotes(Wiley_Interdiscip_Rev_RNA).pdf},
  eprinttype = {pmid},
  eprint = {25346433},
