  shortjournal = {Can. J. For. Res.},
  urldate = {2015-06-25},
  date = {2010-02-01},
  pages = {347-356},
  author = {Bonet, J. A. and Palahí, M. and Colinas, C. and Pukkala, T. and Fischer, C. R. and Miina, J. and Martínez de Aragón, J.},
  file = {/home/brendan/Documents/Uppsala/Documents/References/Statistics/Species Distribution Modeling/Bonet_etal_2010_Modelling_the_production_and_species_richness_of_wild_mushrooms_in_pine_forests(Can._J._For._Res.).pdf}
