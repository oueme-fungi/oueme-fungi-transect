  date = {2018},
  keywords = {microbiota,Bayesian inference,generalized linear mixed models,host-associated,joint species distribution models,microbiome},
  author = {Björk, Johannes R. and Hui, Francis K. C. and O'Hara, Robert B. and Montoya, Jose M.},
  file = {/home/brendan/.mozilla/firefox/uzm114dr.default/zotero/storage/X2MTCX52/mec.html}
}

