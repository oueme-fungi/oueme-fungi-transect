  file = {/home/brendan/Documents/Uppsala/Documents/References/Poretsky_etal_2005_Analysis_of_Microbial_Gene_Transcripts_in_Environmental_Samples(Appl._Environ._Microbiol.).pdf;/home/brendan/.mozilla/firefox/uzm114dr.default/zotero/storage/7R4Y59DB/4121.html},
