  file = {/home/brendan/Documents/Uppsala/Documents/References/Lab Methods/Molecular Biology/PCR/Simon_etal_2005_Heterogeneity_of_intron_presence_or_absence_in_rDNA_genes_of_the_lichen_species(Curr_Genet).pdf}
}

