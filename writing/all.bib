@article{battyAssessmentEcologicalPotential2005,
  title = {Assessment of the Ecological Potential of Mine-Water Treatment Wetlands Using a Baseline Survey of Macroinvertebrate Communities},
  volume = {138},
  issn = {0269-7491},
  url = {http://www.sciencedirect.com/science/article/pii/S0269749105002435},
  doi = {10.1016/j.envpol.2005.04.022},
  abstract = {A baseline survey of macroinvertebrate populations in two mine-water treatment wetlands, one treating a net acidic spoil heap discharge and one a net alkaline ferruginous pumped mine water, was undertaken to assess the potential of these systems to provide habitats for faunal communities. Both wetlands were found to be impoverished in comparison to natural wetlands but did sustain a macroinvertebrate community that could support higher organisms. Wetland size and water quality in terms of pH, conductivity and metal concentrations were found to be important factors in determining the quality of the populations supported. Direct toxicity to organisms was unlikely to be the main cause of lower diversity, but the smothering of organisms via the precipitation of iron hydroxides particularly in the early parts of the treatment systems affected macroinvertebrate communities. The presence of areas of open water within the planted systems was found to be important for providing habitats for macroinvertebrates and this should be both a future design and maintenance consideration for environmental managers.},
  number = {3},
  journaltitle = {Environmental Pollution},
  shortjournal = {Environmental Pollution},
  urldate = {2014-04-12},
  date = {2005-12},
  pages = {412-419},
  keywords = {Constructed wetlands,Environmental Management,Mine water pollution,macroinvertebrates},
  author = {Batty, L. C. and Atkin, L. and Manning, D. A. C.},
  file = {/home/brendan/.mozilla/firefox/uzm114dr.default/zotero/storage/HFHDUIKF/Batty et al. - 2005 - Assessment of the ecological potential of mine-wat.pdf;/home/brendan/.mozilla/firefox/uzm114dr.default/zotero/storage/N3NS8SWW/Batty et al. - 2005 - Assessment of the ecological potential of mine-wat.pdf;/home/brendan/.mozilla/firefox/uzm114dr.default/zotero/storage/96Z5JWRZ/S0269749105002435.html;/home/brendan/.mozilla/firefox/uzm114dr.default/zotero/storage/BASTMCD5/S0269749105002435.html}
