}

@article{baath2003,
  title = {Comparison of Soil Fungal/Bacterial Ratios in a {{pH}} Gradient Using Physiological and {{PLFA}}-Based Techniques},
  volume = {35},
  issn = {0038-0717},
  url = {http://www.sciencedirect.com/science/article/pii/S0038071703001548},
  doi = {10.1016/S0038-0717(03)00154-8},
  abstract = {We have compared the total microbial biomass and the fungal/bacterial ratio estimated using substrate-induced respiration (SIR) in combination with the selective inhibition technique and using the phospholipid fatty acid (PLFA) technique in a pH gradient (3.0–7.2) consisting of 53 mature broad-leaved forest soils. A fungal/bacterial biomass index using the PLFA technique was calculated using the PLFA 18:2ω6,9 as an indicator of fungal biomass and the sum of 13 bacterial specific PLFAs as indicator of the bacterial biomass. Good linear correlation (p{$<$}0.001) was found between the total microbial biomass estimated with SIR and total PLFAs (totPLFA), indicating that 1 mg biomass-C was equivalent to 130nmol totPLFA. Both biomass estimates were positively correlated to soil pH. The fungal/bacterial ratio measured using the selective inhibition technique decreased significantly with increasing pH from about 9 at pH 3 to approximately 2 at pH 7, while the fungal/bacterial biomass index using PLFA measurements tended to increase slightly with increasing soil pH. Good correlation between the soil content of ergosterol and of the PLFA 18:2ω6,9 indicated that the lack of congruency between the two methods in estimating fungal/bacterial ratios was not due to PLFA 18:2ω6,9-related non-fungal structures to any significant degree. Several PLFAs were strongly correlated to soil pH (R2 values {$>$}0.8); for example the PLFAs 16:1ω5 and 16:1ω7c increased with increasing soil pH, while i16:0 and cy19:0 decreased. A principal component analysis of the total PLFA pattern gave a first component that was strongly correlated to soil pH (R2=0.85, p{$<$}0.001) indicating that the microbial community composition in these beech/beech-oak forest soils was to a large extent determined by soil pH.},
  number = {7},
  journaltitle = {Soil Biology and Biochemistry},
  shortjournal = {Soil Biology and Biochemistry},
  urldate = {2018-08-09},
  date = {2003-07-01},
  pages = {955-963},
  keywords = {Ergosterol,Forest soils,Fungal/bacterial ratios,pH Gradient,Phospholipid fatty acid,Substrate-induced respiration},
  author = {Bååth, E. and Anderson, T. -H.},
  file = {/home/brendan/Documents/Uppsala/Documents/References/Lab Methods/PLFA/Bååth_Anderson_2003_Comparison_of_soil_fungal-bacterial_ratios_in_a_pH_gradient_using_physiological(Soil_Biology_and_Biochemistry).pdf;/home/brendan/.mozilla/firefox/uzm114dr.default/zotero/storage/U5HCZMFQ/S0038071703001548.html}
}

@article{ramsey2006,
  title = {Choice of Methods for Soil Microbial Community Analysis: {{PLFA}} Maximizes Power Compared to {{CLPP}} and {{PCR}}-Based Approaches},
  volume = {50},
  issn = {0031-4056},
  url = {http://www.sciencedirect.com/science/article/pii/S0031405606000138},
  doi = {10.1016/j.pedobi.2006.03.003},
  shorttitle = {Choice of Methods for Soil Microbial Community Analysis},
  abstract = {Summary
Polyphasic studies that used phospholipid fatty acid analysis (PLFA) in conjunction with community level physiological profiling (CLPP) or PCR-based molecular methods were analyzed in order to evaluate the power of each strategy to detect treatment effects on soil microbial community structure (MCS). We found no studies where CLPP or PCR-based methods differentiated treatments that were not also differentiated by PLFA. In 14 of 32 studies (44\%), PLFA differentiated treatments that were not resolved by CLPP analysis. In 5 of 25 studies (20\%), PLFA differentiated treatments that were not resolved by PCR-based methods. We discuss PLFA, CLPP, and PCR-based methods with respect to power to discriminate change in MCS versus potential for characterization of underlying population level changes.},
  number = {3},
  journaltitle = {Pedobiologia},
  shortjournal = {Pedobiologia},
  urldate = {2018-08-09},
  date = {2006-07-03},
  pages = {275-280},
  keywords = {PCR,PLFA,Phospholipid fatty acid,CLPP,Community level physiological profiling,Polyphasic},
  author = {Ramsey, Philip W. and Rillig, Matthias C. and Feris, Kevin P. and Holben, William E. and Gannon, James E.},
  file = {/home/brendan/Documents/Uppsala/Documents/References/Lab Methods/PLFA/Ramsey_etal_2006_Choice_of_methods_for_soil_microbial_community_analysis(Pedobiologia).pdf;/home/brendan/.mozilla/firefox/uzm114dr.default/zotero/storage/9CCG7TSB/S0031405606000138.html}
