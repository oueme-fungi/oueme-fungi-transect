@article{dentinger2009,
  langid = {english},
  title = {Rapid and Reliable High-Throughput Methods of {{DNA}} Extraction for Use in Barcoding and Molecular Systematics of Mushrooms: {{DNA BARCODING}}},
  volume = {10},
