  pages = {169-176},
  keywords = {Biodiversity,Taxonomy,Phylogeny,Ribosomal RNA,Alignments,Databases},
  author = {Glöckner, Frank Oliver and Yilmaz, Pelin and Quast, Christian and Gerken, Jan and Beccati, Alan and Ciuprina, Andreea and Bruns, Gerrit and Yarza, Pablo and Peplies, Jörg and Westram, Ralf and Ludwig, Wolfgang},
