  file = {/home/brendan/Documents/Uppsala/Documents/References/Lab Methods/Molecular Biology/PacBio/Griffith_etal_2018_PacBio_library_preparation_using_blunt-end_adapter_ligation_produces(bioRxiv).pdf}
