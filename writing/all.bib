  file = {/home/brendan/Documents/Uppsala/Documents/References/Niche, Function, and Fungal Diversity JC/Kiers_etal_2011_Reciprocal_Rewards_Stabilize_Cooperation_in_the_Mycorrhizal_Symbiosis(Science).pdf;/home/brendan/.mozilla/firefox/uzm114dr.default/zotero/storage/KEEWC3EL/880.html},
