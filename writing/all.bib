  abstract = {Prediction of taxonomy for marker gene sequences such as 16S ribosomal RNA (rRNA) is a fundamental task in microbiology. Most experimentally observed sequences are diverged from reference sequences of authoritatively named organisms, creating a challenge for prediction methods. I assessed the accuracy of several algorithms using cross-validation by identity, a new benchmark strategy which explicitly models the variation in distances between query sequences and the closest entry in a reference database. When the accuracy of genus predictions was averaged over a representative range of identities with the reference database (100\%, 99\%, 97\%, 95\% and 90\%), all tested methods had ≤50\% accuracy on the currently-popular V4 region of 16S rRNA. Accuracy was found to fall rapidly with identity; for example, better methods were found to have V4 genus prediction accuracy of ∼100\% at 100\% identity but ∼50\% at 97\% identity. The relationship between identity and taxonomy was quantified as the probability that a rank is the lowest shared by a pair of sequences with a given pair-wise identity. With the V4 region, 95\% identity was found to be a twilight zone where taxonomy is highly ambiguous because the probabilities that the lowest shared rank between pairs of sequences is genus, family, order or class are approximately equal.},
  journaltitle = {PeerJ},
  shortjournal = {PeerJ},
  urldate = {2019-04-26},
  date = {2018-04-18},
  pages = {e4652},
  author = {Edgar, Robert C.},
  file = {/home/brendan/Documents/Uppsala/Documents/References/Computer Methods/Bioinformatics/Taxonomic Assignment/Edgar_2018_Accuracy_of_taxonomy_prediction_for_16S_rRNA_and_fungal_ITS_sequences(PeerJ).pdf;/home/brendan/.mozilla/firefox/uzm114dr.default/zotero/storage/I3BSBE3M/4652.html}
}

@article{edgar2018a,
  langid = {english},
  title = {Taxonomy Annotation and Guide Tree Errors in {{16S rRNA}} Databases},
  volume = {6},
  issn = {2167-8359},
  url = {https://peerj.com/articles/5030},
  doi = {10.7717/peerj.5030},
  abstract = {Sequencing of the 16S ribosomal RNA (rRNA) gene is widely used to survey microbial communities. Specialized 16S rRNA databases have been developed to support this approach including Greengenes, RDP and SILVA. Most taxonomy annotations in these databases are predictions from sequence rather than authoritative assignments based on studies of type strains or isolates. In this work, I investigated the taxonomy annotations and guide trees provided by these databases. Using a blinded test, I estimated that the annotation error rate of the RDP database is ∼10\%. The branching orders of the Greengenes and SILVA guide trees were found to disagree at comparable rates with each other and with taxonomy annotations according to the training set (authoritative reference) provided by RDP, indicating that the trees have comparable quality. Pervasive conflicts between tree branching order and type strain taxonomies strongly suggest that the guide trees are unreliable guides to phylogeny. I found 249,490 identical sequences with conflicting annotations in SILVA v128 and Greengenes v13.5 at ranks up to phylum (7,804 conflicts), indicating that the annotation error rate in these databases is ∼17\%.},
  journaltitle = {PeerJ},
  shortjournal = {PeerJ},
  urldate = {2019-04-26},
  date = {2018-06-12},
  pages = {e5030},
  author = {Edgar, Robert},
  file = {/home/brendan/Documents/Uppsala/Documents/References/Computer Methods/Bioinformatics/Taxonomic Assignment/Edgar_2018_Taxonomy_annotation_and_guide_tree_errors_in_16S_rRNA_databases(PeerJ).pdf;/home/brendan/.mozilla/firefox/uzm114dr.default/zotero/storage/L8KJTS8I/5030.html}
}

