  langid = {english},
  title = {Microsatellite Markers Reveal the below Ground Distribution of Genets in Two Species of {{Rhizopogon}} Forming Tuberculate Ectomycorrhizas on {{Douglas}} Fir},
  volume = {161},
  issn = {1469-8137},
