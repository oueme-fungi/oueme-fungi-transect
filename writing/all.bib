@article{thiereWetlandCreationAgricultural2009,
  title = {Wetland Creation in Agricultural Landscapes: {{Biodiversity}} Benefits on Local and Regional Scales},
  volume = {142},
  issn = {0006-3207},
  url = {http://www.sciencedirect.com/science/article/pii/S0006320709000329},
  doi = {10.1016/j.biocon.2009.01.006},
  shorttitle = {Wetland Creation in Agricultural Landscapes},
  abstract = {Wetland creation aiming at a simultaneous increase in nutrient retention and species diversity in agricultural landscapes has recently become applied as a catchment-scale compensation measure for past wetland losses. Here, we evaluate if, and to what extend, dual-purpose wetlands benefit local and regional diversity of agricultural landscapes. We analysed composition and α, β, and γ diversity of aquatic macroinvertebrate assemblages among dual-purpose wetlands in an agricultural region in southwest Sweden in relation to local (water quality, wetland morphology, succession stage, proximity to other aquatic habitats) and landscape parameters (regional connectivity, wetland density). Diversity of mature agricultural ponds was used as a standard to evaluate the value of dual-purpose wetlands. Dual-purpose wetlands sustained α, β, and γ diversity similar to that of natural lentic water bodies in agricultural landscapes in the region and elsewhere. Over 80\% of the overall species richness was attributed to β diversity, and each created wetland contributed to overall species accumulation. Ecosystem parameters explained 19\% of the compositional variation among assemblages, but were only marginally related to diversity. Wetland density promoted α and γ diversity, while spatial heterogeneity (β) remained equally high, independent of wetland density. Our results indicate that catchment-scale wetland creation for simultaneous retention and diversity purposes benefits the biodiversity of agricultural landscapes, particularly if the density of aquatic habitats is increased by at least 30\%.},
  number = {5},
  journaltitle = {Biological Conservation},
  shortjournal = {Biological Conservation},
  urldate = {2014-04-13},
  date = {2009-05},
  pages = {964-973},
  keywords = {Aquatic habitat density,Assemblage composition,Benthic macroinvertebrates,Catchment restoration,species richness,ecosystem services},
  author = {Thiere, Geraldine and Milenkovski, Susann and Lindgren, Per-Eric and Sahlén, Göran and Berglund, Olof and Weisner, Stefan E. B.},
  file = {/home/brendan/.mozilla/firefox/uzm114dr.default/zotero/storage/SB5EW4KE/Thiere et al. - 2009 - Wetland creation in agricultural landscapes Biodi.pdf;/home/brendan/.mozilla/firefox/uzm114dr.default/zotero/storage/2H28R4PH/S0006320709000329.html}
}

