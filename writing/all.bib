  shortjournal = {Can. J. Bot.},
  urldate = {2015-06-16},
  date = {2004-12-01},
  pages = {1711-1729},
  author = {Richard, F and Moreau, P -A and Selosse, M -A and Gardes, M},
  file = {/home/brendan/Documents/Uppsala/Documents/References/Fruitbodies/Richard_etal_2004_Diversity_and_fruiting_patterns_of_ectomycorrhizal_and_saprobic_fungi_in_an(Can._J._Bot.).pdf}
